    "karma-phantomjs-launcher": "~0.2.1",
    "karma-requirejs": "~0.2.2",
    "karma-safari-launcher": "0.1.1",
    "karma-script-launcher": "~0.1.0",
    "protractor": "~2.5.1",
    "selenium-webdriver": "~2.48.0"
  }
