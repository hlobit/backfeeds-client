{
  "name": "backfeeds-client",
  "version": "0.1.0",
  "description": "Collect the feedbacks for your web product.",
  "scripts": {
    "postinstall": "webdriver-manager update && bower install",
    "start": "brunch w -s",
    "dev": "brunch w",
    "prod": "rm -rf public && brunch b -P && ./deploy.sh && rm -rf public",
    "test": "karma start test/karma.conf.js",
    "test-e2e": "protractor test/protractor-conf.js"
  },
