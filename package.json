  "bugs": {
    "url": "https://github.com/hlobit/backfeeds-client/issues"
  },
  "homepage": "https://github.com/hlobit/backfeeds-client",
  "dependencies": {
  },
  "devDependencies": {
    "bower": "~1.6.3",
    "brunch": "~1.8.5",
	  "browser-sync-brunch": "~0.0.9",
	  "sass-brunch": "~1.9.1",
	  "postcss-brunch": "~0.5.0",
	  "css-brunch": "~1.7.0",
    "clean-css-brunch": "~1.7.2",
	  "ng-annotate-brunch": "~1.7.2",
	  "javascript-brunch": "~1.8.0",
	  "uglify-js-brunch": "~1.7.8",
